<div id="container" class="containerEntrada">
    <div class="container1Entrada">
        <div>
            <a class="btn btn-success" href="/Entrada/Create"> Novo</a>
            <a class="btn btn-primary" href="https://localhost:7215/"> Voltar</a>
        </div>
    </div>
    <div class="container2Entrada">
        <table class="TableContainerEntrada">
            <thead class="theadEntrada">
            <th>
                Código
            </th>
            <th>
                Descrição
            </th>
            <th>
                Quantidadeatual
            </th>
            <th>
                DataEntrada
            </th>
            </thead>
            <tbody>
                @if (ViewBag.Produto != null){
                @foreach (var item in ViewBag.Entrada)
                {
                    <tr>
                        <td class="tdEntrada">@item.codigo</td>
                        <td class="tdEntrada">@item.descricao</td>
                        <td class="tdEntrada">@item.quantidadeatual</td>
                        <td class="tdEntrada">@item.dataentrada</td>
                        <td class="tdEntrada">
                            <div class="IconesContainerEntrada">
                                <img class="imagenslixo" src="/Imagens/trash.svg" alt="Excluir" />
                                <img class="imagenseditar" src="/Imagens/pencil-square.svg" alt="Editar" />
                            </div>
                        </td>
                    </tr>

                }
                }
            </tbody>


        </table>
    </div>
    <div class="container3Entrada">
        <div>
            
        </div>
    </div>
</div>

