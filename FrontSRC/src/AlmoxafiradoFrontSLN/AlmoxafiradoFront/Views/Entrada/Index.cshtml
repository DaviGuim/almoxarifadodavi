<div id="container" class="containerEnt">
    <div class="container1Ent">
        <div>
            <a class="btnEnt" href="/Entrada/Create"> Novo
            </a>
        </div>
    </div>
    <div class="container2Ent">
        <table class="TableContainerEnt">
            <thead class="theadEnt">
            <th>
                Código
            </th>
            <th>
                Data de Entrada
            </th>
            <th>
                Codigo do fornecedor
            </th>
            <th>
                Codigo do Produto
            </th>
            <th>
                Quantidade
            </th>
            <th>
                Observações
            </th>
            <th>
                Opções
            </th>
            </thead>
            <tbody>

                @foreach (var item in ViewBag.Entradas)
                {
                    <tr>
                        <td class="tdEnt">@item.codigo</td>
                        <td class="tdEnt">@item.dataEntrada</td>
                        <td class="tdEnt">@item.codigoFronecedor</td>
                        <td class="tdEnt">@item.codigoProduto</td>
                        <td class="tdEnt">@item.quantidade</td>
                        <td class="tdEnt">@item.observacao</td>
                        <td class="tdEnt">
                            <div class="IconesContainerEnt">
                                <div class="Icone1Ent">
                                    <button class="trash-btn">
                                        <div class="fas fa-trash"></div>
                                    </button>
                                </div>
                                <div class="Icone2Ent">
                                    <button class="edit-button">
                                        <div class="fas fa-edit"></div>
                                    </button>
                                </div>
                            </div>
                        </td>
                    </tr>

                }
            </tbody>


        </table>
    </div>
    <div class="container3Ent">
        <div>
            <a class="btnPro " href="/">
                Voltar
            </a>
        </div>
    </div>
</div>

