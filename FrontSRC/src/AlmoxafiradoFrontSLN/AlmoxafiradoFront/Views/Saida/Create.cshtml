<h1>Cadastro Saida</h1>

<form method=" post" action="/Saida/Cadastrar">
    <div class="mb-3">
        <label for=" descricao" class="form-label">Descrição</label>
        <input type="text" class="form-control" id=" descricao" name=" descricao" maxlength="50">
    </div>
    <div class="mb-3">
        <label for=" quantatual" class="form-label">Quantidade Atual</label>
        <input type="number" class="form-control" id=" quantatual" name=" quantatual" maxlength="1000">
    </div>
    <div class="mb-3">
        <label for="quantsaida" class="form-label">Quantidade de Saída</label>
        <input type="number" class="form-control" id=" quantsaida" name=" quantsaida" maxlength="1000">
    <div/>



    <button class="btn btn-primary"> Voltar</button>
    <button type="submit" class="btn btn-primary"> Salvar</button>

</form>